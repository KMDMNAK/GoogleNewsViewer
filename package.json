{
    "name": "googlenewsviewer",
    "displayName": "Google News Viewer",
    "description": "you can read Google news on vscode",
    "version": "1.0.0",
    "icon":"google-news-vscode-logo.png",
    "engines": {
        "vscode": "^1.37.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "googlenews"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./dist/main.entry.js",
    "contributes": {
        "commands": [
            {
                "command": "googlenews.view.search.query",
                "title": "Google News:Search Query"
            },
            {
                "command": "googlenews.view.search.geo",
                "title": "Google News:Search Geo"
            },
            {
                "command": "googlenews.view.search.topic",
                "title": "Google News:Search Topic"
            }
        ],
        "configuration": {
            "title": "GoogleNews",
            "properties": {
                "googleNews.language": {
                    "type": "string",
                    "default": "UK_ENGLISH",
                    "enum": [
                        "JAPANESE",
                        "US_ENGLISH",
                        "UK_ENGLISH",
                        "CHINESE",
                        "DEUTSCH",
                        "SPANISH",
                        "ARABIC"
                    ]
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compileMain && npm run compileWebview",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js",
        "compileWebviewWatch": "webpack --mode production --config webpack.config.view.js --watch",
        "compileWebview": "webpack --mode production --config webpack.config.view.js",
        "compileMain": "webpack --mode production --config webpack.config.src.js"
    },
    "devDependencies": {
        "@material-ui/core": "^4.4.1",
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.7",
        "@types/moment": "^2.13.0",
        "@types/node": "^12.7.4",
        "@types/node-fetch": "^2.5.0",
        "@types/react": "^16.9.2",
        "@types/react-dom": "^16.9.0",
        "@types/vscode": "^1.37.0",
        "css-loader": "^3.2.0",
        "glob": "^7.1.4",
        "mocha": "^6.2.0",
        "ts-loader": "^6.0.4",
        "tslint": "^5.19.0",
        "typescript": "^3.6.2",
        "vscode-test": "^1.2.0",
        "webpack": "^4.39.3",
        "webpack-cli": "^3.3.8",
        "webpack-merge": "^4.2.2"
    },
    "dependencies": {
        "@date-io/date-fns": "^1.3.11",
        "@date-io/moment": "^1.3.11",
        "@material-ui/pickers": "^3.2.5",
        "express": "^4.17.1",
        "express-react-views": "^0.11.0",
        "moment": "^2.24.0",
        "node-fetch": "^2.6.0",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "rss-parser": "^3.7.2"
    },
    "publisher": "GoogleNewsViewer",
    "repository": {
        "type": "git",
        "url": "https://github.com/KMDMNAK/GoogleNewsViewer.git"
    }
}
